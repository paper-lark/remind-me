{
  "name": "remind-me",
  "version": "1.0.7",
  "description": "Simplistic cross-platform reminder app",
  "main": "./source/main/main.js",
  "scripts": {
    "start": "yarn run electron .",
    "watch": "yarn run gulp watch",
    "prebuild": "yarn run gulp",
    "build:test": "yarn run electron-builder --dir --config electronrc.json",
    "build:mac": "yarn run electron-builder --config electronrc.json --mac",
    "build:windows": "yarn run electron-builder --config electronrc.json --win",
    "build:linux": "yarn run electron-builder --config electronrc.json --linux",
    "build": "yarn run electron-builder --config electronrc.json -mwl"
  },
  "author": "Max Zhuravsky <paperlark@yandex.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.7.10",
    "electron-builder": "^19.54.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-minify": "^2.1.0",
    "gulp-ruby-sass": "^3.0.0",
    "icon-gen": "^1.2.0"
  },
  "dependencies": {}
}
